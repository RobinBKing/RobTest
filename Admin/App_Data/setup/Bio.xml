  <WebBackRow>
    <Configuration>
      <Label>Education</Label>
    </Configuration>
    <WebBackFields>
      <WebBackField>
        <Type>association</Type>
        <Associations>
          <Association objectname="EducationLevel">
            <AssociationConfiguration>
              <Rankable>True</Rankable>
              <Detailed>True</Detailed>
              <Name>Education</Name>
              <DetailFields>
                <WebBackField>
                  <Title>School</Title>
                  <DBField>School</DBField>
                  <Type>WebBack.School</Type>
                </WebBackField>
                <WebBackField>
                  <Title>Note</Title>
                  <DBField>Note</DBField>
                  <Type>string</Type>
                  <MaxLength>255</MaxLength>
                  <Size>50</Size>
                </WebBackField>
                <WebBackField>
                  <Title>Degree</Title>
                  <DBField>Degree</DBField>
                  <Type>string</Type>
                  <MaxLength>255</MaxLength>
                  <Size>50</Size>
                </WebBackField>
                <WebBackField>
                  <Title>Year</Title>
                  <DBField>Year</DBField>
                  <Type>string</Type>
                  <MaxLength>10</MaxLength>
                  <Size>10</Size>
                </WebBackField>
              </DetailFields>
            </AssociationConfiguration>
          </Association>
        </Associations>
      </WebBackField>
    </WebBackFields>    
